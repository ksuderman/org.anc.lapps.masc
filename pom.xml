<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <artifactId>war-parent-pom</artifactId>
        <groupId>org.lappsgrid.maven</groupId>
        <version>1.2.0</version>
    </parent>
    <name>MASC DataSource</name>
    <description>LAPPS DataSource for service MASC files.</description>
    <groupId>org.anc.lapps.masc</groupId>
    <artifactId>masc-datasource</artifactId>
    <packaging>war</packaging>
    <version>1.4.1</version>
    <properties>
        <lapps.version>1.0.1</lapps.version>
        <lapps.core.version>1.1.0</lapps.core.version>
    </properties>
    <build>
        <finalName>MascDataSource#${project.version}</finalName>
    </build>
    <dependencies>
        <dependency>
            <!-- Groovy is used for some code in the test directory. -->
            <groupId>org.codehaus.groovy</groupId>
            <artifactId>groovy-all</artifactId>
            <version>2.1.1</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.anc.lapps</groupId>
            <artifactId>core</artifactId>
            <version>0.2.1</version>
        </dependency>
        <dependency>
            <groupId>org.lappsgrid</groupId>
            <artifactId>discriminator</artifactId>
            <version>1.2.2</version>
            <type>jar</type>
            <scope>compile</scope>
        </dependency>
        <dependency>
            <groupId>org.lappsgrid</groupId>
            <artifactId>api</artifactId>
            <version>${lapps.version}</version>
        </dependency>
        <dependency>
            <groupId>org.lappsgrid</groupId>
            <artifactId>core</artifactId>
            <version>${lapps.core.version}</version>
        </dependency>
        <dependency>
            <groupId>org.anc.lapps</groupId>
            <artifactId>serialization</artifactId>
            <version>0.12.1</version>
        </dependency>
    </dependencies>
</project>